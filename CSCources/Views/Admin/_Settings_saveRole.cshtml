
<strong>@TempData["user_Email"] </strong>

<hr />

@foreach (var item in ViewBag.roles)
{
    using (Ajax.BeginForm("_Settings_delRole", "Admin", new { id = @TempData["user_Id"], role = item, user_Email = @TempData["user_Email"] }, new AjaxOptions() { UpdateTargetId = "settings" }))
    {
        <input type="submit" value="Удалить роль" class="btn btn-danger" />
        <strong>@item</strong>
    }
    <br />

}

<hr />


